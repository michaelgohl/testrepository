<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    xmlns:ems="http://schemas.android.com/apk/res/de.guj.ems.test.app">
    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/FragmentVideoPlayButton"
        android:id="@+id/FragmentVideoStart"
        android:layout_gravity="center_horizontal" />

    <de.guj.ems.mobile.sdk.views.video.GuJEMSVideoPlayer
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:gravity="center"
        android:id="@+id/FragmentVideoPlayer"
        />

</LinearLayout>
